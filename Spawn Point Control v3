//GMS v174.2 - Updated by RealYaoMing
//Spawn Point Control
Define(SpawnX,#0) //X Coord
Define(SpawnY,#0) //Y Coord
[Enable]
Alloc(Hook,128)
label(returnhere)
label(originalcode)
label(exit)

Hook:
mov edi,SpawnX
mov ebx,SpawnY
originalcode:
call 018CC620 //Original OpCode

exit:
jmp returnhere

017EE543:
jmp Hook
returnhere:

[Disable]
017EE543: //E8 ? ? ? ? 8B 85 ? ? ? ? 8B ? 8B 52 ? 6A ? 50 6A ? 6A ? 83 C3 ? 53 57 6A ? 8B ? FF ? 6A ? 6A
call 018CC620

DeAlloc(Hook)