define(RLA,0051779A)//81 ? ? ? 00 00 0F 8D ? ? 00 00 85 ? 0F 84  [1st result]
define(TUB,01981628)//8D 8E ? ? 00 00 E8 ? ? ? ? E8 ? ? ? ? 50
//176.1
[ENABLE]
alloc(Tubi,64)
label(Ret)

Tubi:
mov eax, 0
push esi
jmp Ret

TUB:
jmp Tubi
Ret:

RLA: // Remove loot animation
db 81 FE 00 00 00 00

[DISABLE]
TUB: // FUNCTION START (mov eax,[esp+04]): 8D 8E ? ? 00 00 E8 ? ? ? ? E8 ? ? ? ? 50
mov eax,[esp+04]
