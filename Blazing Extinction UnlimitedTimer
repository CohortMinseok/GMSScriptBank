define(UBET,00A05790)//89 86 ? ? ? ? 89 9e ? ? ? ? 89 9e ? ? ? ? e8 ? ? ? ? 83 ? ? 85 c0

//177.2

[enable]
alloc(SkillTimer,128)
label(AppearUnlimited)

UBET:
jmp SkillTimer
db 90
AppearUnlimited:

SkillTimer:
mov [esi+000000FC],7FFFFFFF
jmp AppearUnlimited

[disable]
UBET:
mov [esi+000000FC],eax

