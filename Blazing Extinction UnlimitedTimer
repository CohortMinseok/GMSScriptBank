define(UBET,009A6E6A)//89 86 ? ? ? ? 89 9e ? ? ? ? 89 9e ? ? ? ? e8 ? ? ? ? 83 ? ? 85 c0

//176.2

[enable]
alloc(SkillTimer,128)
label(AppearUnlimited)

UBET:
jmp SkillTimer
db 90
AppearUnlimited:

SkillTimer:
mov [esi+000000F8],7FFFFFFF
jmp AppearUnlimited

[disable]
UBET:
mov [esi+000000F8],eax
